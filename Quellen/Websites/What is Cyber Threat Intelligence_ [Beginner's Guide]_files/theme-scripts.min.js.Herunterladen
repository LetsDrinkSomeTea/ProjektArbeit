eval("let reports = document.querySelectorAll('.featured_report_section .single_report');\nlet shareButton = document.querySelectorAll('.share-button');\nlet closeButton = document.querySelectorAll('.close-button');\n\nconst showSocial = event => {\n    if (!event.currentTarget.nextElementSibling.classList.contains(\"open\")) {\n        event.currentTarget.nextElementSibling.classList.add(\"open\");\n    } else {\n        event.currentTarget.nextElementSibling.classList.remove(\"open\");\n    }\n};\n\nconst closeSocial = event => {\n    event.currentTarget.closest('.share-buttons').classList.remove('open');\n};\n\nif (reports.length > 0) {\n    for (let i = 0; i < reports.length; i++) {\n        if (shareButton[i]) {\n            shareButton[i].addEventListener('click', showSocial);\n        }\n        if (closeButton[i]) {\n            closeButton[i].addEventListener('click', closeSocial);\n        }\n    }\n}\n\n//# sourceURL=webpack:///./scripts/validation.js?");

