"use strict";
eval("__webpack_require__.r(__webpack_exports__);\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"swapCopyByLang\", function() { return swapCopyByLang; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"checkIfPropExistsInArray\", function() { return checkIfPropExistsInArray; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"capitalizeFirstLetter\", function() { return capitalizeFirstLetter; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"addSpacesBetweenCaps\", function() { return addSpacesBetweenCaps; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"convertToUppercaseWords\", function() { return convertToUppercaseWords; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"randomText\", function() { return randomText; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"chunk\", function() { return chunk; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"debounce\", function() { return debounce; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"isWithinBreakpoint\", function() { return isWithinBreakpoint; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"scrambleTextInElement\", function() { return scrambleTextInElement; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"unscrambleTextInElement\", function() { return unscrambleTextInElement; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"setGate\", function() { return setGate; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"createLoader\", function() { return createLoader; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"handleModalOpen\", function() { return handleModalOpen; });\n/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, \"getTranslatedValue\", function() { return getTranslatedValue; });\n/**\n * Swap copy to translated version based on provided language\n * @param {string} lang - The language to swap to\n * @param {string} dText - default language to use if condition matches\n * @param {string} tText - translated copy to use if condition matches\n */\nfunction swapCopyByLang(lang, { dText, tText }) {\n    if (!dText) return;\n    return document.documentElement.lang.toLowerCase() === lang.toLowerCase() ? tText : dText;\n}\n\n/**\n ** Check if the provided property exists in an array of objects\n * @param {array} arr - The array to check\n * @param {string} propName - The property name to check\n * @param {string} value - The value to check for\n * @example\n * checkIfPropExistsInArray([{ id: 1, name: 'John' }, { id: 2, name: 'Jane' }], 'name', 'John');\n * @returns {boolean} - Returns true if the property exists in the array\n * @returns {boolean} - Returns false if the property does not exist in the array\n * @returns {null} - Returns null if no array is provided\n */\nconst checkIfPropExistsInArray = (arr, propName, value) => arr.some(item => item[propName] === value);\n\n/**\n ** Capitalize the first letter of a string\n * @param {string} str - The string to capitalize\n * @example\n * capitalizeFirstLetter('hello world');\n * @returns {string} - Returns the capitalized string\n * @returns {null} - Returns null if no string is provided\n */\nconst capitalizeFirstLetter = str => str.charAt(0).toUpperCase() + str.slice(1);\n\n/**\n ** Add spaces between capital letters in a string\n * @param {string} str - The string to add spaces to\n * @example\n * addSpacesBetweenCaps('helloWorld');\n * @returns {string} - Returns the string with spaces between capital letters\n * @returns {null} - Returns null if no string is provided\n */\nconst addSpacesBetweenCaps = str => str.replace(/([A-Z])/g, ' $1').trim();\n\n/**\n ** Convert a string to uppercase words\n * @param {string} str - The string to convert\n * @example\n * convertToUppercaseWords('hello world');\n * @returns {string} - Returns the converted string\n * @returns {null} - Returns null if no string is provided\n */\nconst convertToUppercaseWords = str => str.toLowerCase().split(' ').map(s => s.charAt(0).toUpperCase() + s.substring(1)).join(' ');\n\n/**\n ** Get a random number between a min and max value\n * @param {number} min - The minimum value\n * @param {number} max - The maximum value\n * @example\n * getRandomNumber(1, 10);\n * @returns {number} - Returns a random number between the min and max values\n * @returns {null} - Returns null if no min or max values are provided\n */\nconst randomText = () => Math.random().toString(36).substring(2, 8) + Math.random().toString(36).substring(2, 8);\n\n/**\n ** Break an array into chunks of a specified size\n * @param {array} arr - The array to chunk\n * @param {number} size - The size of each chunk\n * @example\n * chunk([1, 2, 3, 4, 5, 6, 7, 8, 9], 3);\n * @returns {array} - Returns the chunked array\n * @returns {array} - Returns an empty array if no array is provided\n */\nconst chunk = (arr, size) => Array.from({ length: Math.ceil(arr.length / size) }, (_, i) => arr.slice(i * size, i * size + size));\n\n/**\n ** Debounce a function\n * @param {function} func - The function to debounce\n * @param {number} timeout - The timeout in milliseconds\n * @param {object} opts - The options object\n * @param {function} opts.startFunc - The function to run before the timeout\n * @param {function} opts.endFunc - The function to run after the timeout\n * @example\n * debounce(() => console.log('Hello world'), 300, { startFunc: () => console.log('Starting'), endFunc: () => console.log('Ending'), });\n * @returns {function} - Returns the debounced function\n */\nfunction debounce(func, timeout = 300, opts = {}) {\n    let timer;\n    const { startFunc = () => {}, endFunc = () => {} } = opts;\n\n    return (...args) => {\n        clearTimeout(timer);\n        startFunc();\n\n        timer = setTimeout(() => {\n            func.apply(this, args);\n            endFunc();\n        }, timeout);\n    };\n}\n\n/**\n ** Check if the current window width is within the provided breakpoint\n * @param {number} width - The breakpoint width to check against\n * @example\n * isWithinBreakpoint(768);\n * @returns {boolean} - Returns true if the window width is within the breakpoint\n * @returns {null} - Returns null if no width is provided\n * @returns {boolean} - Returns false if the window width is not within the breakpoint\n */\nfunction isWithinBreakpoint(width) {\n    if (!!!width) return null;\n\n    if (matchMedia) {\n        const mqWidth = window.matchMedia(`(max-width: ${width - 1}px)`);\n        return mqWidth.matches;\n    } else {\n        return window.innerWidth < width;\n    }\n}\n\n/**\n ** Recursively get all text nodes from within provided element and it's child elements and\n * scramble them\n * @param {HTMLElement} element - The element to scramble text in\n * @example\n * scrambleTextInElement(document.querySelector('.form-wrapper'));\n */\nfunction scrambleTextInElement(element) {\n    let children = element.childNodes;\n    const data = [];\n\n    for (let i = 0; i < children.length; i++) {\n        let child = children[i];\n\n        if (child.nodeType === Node.TEXT_NODE) {\n            let randomText = randomText(10);\n            const isMatch = data.find(item => item.text === child.textContent);\n            if (!isMatch) data.push({ text: child.textContent, randomText });\n            child.textContent = randomText;\n        } else {\n            scrambleTextInElement(child);\n        }\n    }\n\n    return data;\n}\n\n/**\n ** Recursively get all text nodes from within provided element and it's child elements and\n * unscramble them using the provided data array\n * @param {HTMLElement} element - The element to unscramble text in\n * @param {array} data - The data array containing the text and random text\n * @example\n * unscrambleTextInElement(document.querySelector('.form-wrapper'), data);\n * @example\n * unscrambleTextInElement(document.querySelector('.form-wrapper'), [{ text: 'Submit', randomText: 'sdfgdfg' }, { text: 'Submit', randomText: 'sdfgdfg' } ]);\n */\nfunction unscrambleTextInElement(element, data) {\n    let children = element.childNodes;\n\n    for (let i = 0; i < children.length; i++) {\n        let child = children[i];\n\n        if (child.nodeType === Node.TEXT_NODE) {\n            const matchedText = data.find(item => item.randomText === child.textContent);\n            if (!!matchedText) child.textContent = matchedText.text;\n        } else {\n            unscrambleTextInElement(child);\n        }\n    }\n}\n\n/**\n ** Set the gate status of all gated items on the page\n * @param {boolean} status - The status to set the gate to\n * @example\n * setGate(true);\n */\nfunction setGate(status) {\n    const gatedItems = document.querySelectorAll('[data-gt]');\n    gatedItems.forEach(gatedItem => gatedItem.dataset.gt = !!!status);\n}\n\n/**\n ** Create a loader element and append it to the provided wrapper element\n * @param {HTMLElement} wrapper - The wrapper element to append the loader to\n * @param {object} opts - Options object\n * @param {string} opts.color - The color of the loader\n * @example\n * createLoader(document.querySelector('.form-wrapper'), { color: 'dark' });\n */\nfunction createLoader(wrapper, opts = {}) {\n    const color = opts.color || 'red';\n    const rippleWrapper = document.createElement('div');\n    const rippleInner1 = document.createElement('div');\n    const rippleInner2 = document.createElement('div');\n    rippleWrapper.classList.add('lds-ripple');\n    rippleWrapper.dataset.color = color;\n    rippleWrapper.append(rippleInner1, rippleInner2);\n    wrapper.appendChild(rippleWrapper);\n}\n\n/**\n ** Modal open event handler\n * @param {Event} e - The event object\n * @param {HTMLElement} wrapper - The wrapper element to append the loader to\n * @param {HTMLElement} closeBtn - The modal's close button\n */\nfunction handleModalOpen(e, wrapper, closeBtn) {\n    wrapper.dataset.status = 'active';\n    document.addEventListener('keydown', e => handleModalClose(e, wrapper, closeBtn));\n    if (!!wrapper) wrapper.addEventListener('click', e => handleModalClose(e, wrapper, closeBtn), false);\n    if (!!closeBtn) closeBtn.addEventListener('click', e => handleModalClose(e, wrapper, closeBtn), false);\n\n    function handleModalClose(e, wrapper, closeBtn) {\n        const isContainer = e.target.classList.contains('modal--container');\n        const isClose = !!e.target.classList ? e.target.classList.contains('fa-close') : null;\n        const shouldExit = !!isContainer || !!isClose || e.keyCode === 27;\n\n        if (shouldExit) {\n            wrapper.dataset.status = 'inactive';\n            wrapper.removeEventListener('click', handleModalClose);\n            closeBtn.removeEventListener('click', handleModalClose);\n            document.removeEventListener('keydown', handleModalClose);\n            wrapper.querySelector('.content-container').innerHTML = '';\n        }\n    }\n}\n\n/** Get the translated value if not on US site\n * @param {string} phrase - Default English phrase to use if on US site\n * @param {string} translatedPhrase - Translated phrase to use if not on US site\n */\nfunction getTranslatedValue(phrase, translatedPhrase) {\n    const locale = document.querySelector('html').getAttribute('country');\n    if (locale === 'ja') return translatedPhrase;\n    return phrase;\n}\n\n//# sourceURL=webpack:///./scripts/components/general-helpers.js?");

