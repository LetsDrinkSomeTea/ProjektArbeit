\n  if (currentPageNumber > 4) {\n    // first element\n    createLiElement(paginationUL, \"1\", 1, category_slug);\n    //li for ... on left\n    if (currentPageNumber > 5) {\n      let curLi = document.createElement(\"li\");\n      let curSpan = document.createElement(\"span\");\n      curSpan.setAttribute(\"class\", \"page-numbers dots\");\n      curSpan.innerHTML = \"...\";\n      curLi.appendChild(curSpan);\n      paginationUL.appendChild(curLi);\n    }\n  }\n\n  for (let i = currentPageNumber - 3; i <= currentPageNumber + 3 && i <= lastPageNumber; i++) {\n    if (i > 0 && i != currentPageNumber) {\n      createLiElement(paginationUL, String(i), i, category_slug);\n    }\n    if (i == currentPageNumber) {\n      let curLi = document.createElement(\"li\");\n      let curLink = document.createElement(\"a\");\n      curLink.setAttribute(\"class\", \"page-numbers current-page-num\");\n      curLink.innerHTML = i;\n      curLi.appendChild(curLink);\n      paginationUL.appendChild(curLi);\n    }\n  }\n\n  if (lastPageNumber - 3 > currentPageNumber) {\n    // ...\n    if (lastPageNumber - 4 > currentPageNumber) {\n      let curLi = document.createElement(\"li\");\n      let curSpan = document.createElement(\"span\");\n      curSpan.setAttribute(\"class\", \"page-numbers dots\");\n      curSpan.innerHTML = \"...\";\n      curLi.appendChild(curSpan);\n      paginationUL.appendChild(curLi);\n    }\n    // last page li\n    createLiElement(paginationUL, String(lastPageNumber), lastPageNumber, category_slug);\n  }\n\n  if (currentPageNumber < lastPageNumber) {\n    //li for next page\n    createLiElement(paginationUL, \">\", currentPageNumber + 1, category_slug);\n  }\n}\n\nfunction createPageButtonsMobile(currentPageNumber, category_slug, paginationUL, lastPageNumber) {\n  console.log(\"Mobile Pg Btn\");\n  if (currentPageNumber > 1) {\n    //li for go back\n    createLiElement(paginationUL, \"<\", currentPageNumber - 1, category_slug);\n  }\n\n  if (currentPageNumber > 2) {\n    // first element\n    createLiElement(paginationUL, \"1\", 1, category_slug);\n    //li for ... on left\n    if (currentPageNumber > 3) {\n      let curLi = document.createElement(\"li\");\n      let curSpan = document.createElement(\"span\");\n      curSpan.setAttribute(\"class\", \"page-numbers dots\");\n      curSpan.innerHTML = \"...\";\n      curLi.appendChild(curSpan);\n      paginationUL.appendChild(curLi);\n    }\n  }\n\n  for (let i = currentPageNumber - 1; i <= currentPageNumber + 1 && i <= lastPageNumber; i++) {\n    if (i > 0 && i != currentPageNumber) {\n      createLiElement(paginationUL, String(i), i, category_slug);\n    }\n    if (i == currentPageNumber) {\n      let curLi = document.createElement(\"li\");\n      let curLink = document.createElement(\"a\");\n      curLink.setAttribute(\"class\", \"page-numbers current-page-num\");\n      curLink.innerHTML = i;\n      curLi.appendChild(curLink);\n      paginationUL.appendChild(curLi);\n    }\n  }\n\n  if (lastPageNumber - 1 > currentPageNumber) {\n    // ...\n    if (lastPageNumber - 2 > currentPageNumber) {\n      let curLi = document.createElement(\"li\");\n      let curSpan = document.createElement(\"span\");\n      curSpan.setAttribute(\"class\", \"page-numbers dots\");\n      curSpan.innerHTML = \"...\";\n      curLi.appendChild(curSpan);\n      paginationUL.appendChild(curLi);\n    }\n    // last page li\n    createLiElement(paginationUL, String(lastPageNumber), lastPageNumber, category_slug);\n  }\n\n  if (currentPageNumber < lastPageNumber) {\n    //li for next page\n    createLiElement(paginationUL, \">\", currentPageNumber + 1, category_slug);\n  }\n}\n\n// list elements for pagination buttons\nfunction createLiElement(paginationUL, pageText, pageDestination, category_slug) {\n  let curLi = document.createElement(\"li\");\n  let curLink = document.createElement(\"a\");\n  curLink.setAttribute(\"class\", \"page-numbers\");\n  // curLink.setAttribute(\"onClick\", 'setWindowHash(\"' +