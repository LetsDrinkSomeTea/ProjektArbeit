eval("/* WEBPACK VAR INJECTION */(function($) {// To set up link animation:\n// 1) Add data-click-animate=\"1\" attribute to a link. Eg: <a data-click-animate=\"1\">some text</a>\n// 2) Set href attribute to a value that would match section <data-click-animate> attribute. Eg: <a data-click-animate=\"1\" href=\"#section_to_scroll\">some text</a>\n// 3) Set attribute data-click-animate to html section. Eg: <section data-click-animate=\"#section_to_scroll\"></section>\n\n\n$(document).ready(function () {\n\n\tif ($('[data-click-animate]').length) {\n\n\t\tlet animatedObj = {};\n\n\t\t// The link as it's displayed in the address bar. Will have a hash if coming from an anchor click on another page.\n\t\tlet addressBarLink = window.location.href;\n\n\t\t// If user clicks an anchor link from a different page, check URL for a hash then run the animate scroll function.\n\t\tif (window.location.hash) {\n\t\t\taddressBarLink = window.location.pathname + window.location.hash;\n\t\t\tanimateScrollPosition(addressBarLink);\n\t\t}\n\n\t\t$('a[data-click-animate=\"1\"]').on('click', function (e) {\n\t\t\tlet targetLink = $(this).attr('href');\n\n\t\t\t// Rebuild the page link to exclude the hash\n\t\t\taddressBarLink = window.location.origin + window.location.pathname;\n\n\t\t\tif (targetLink.includes(\"#\")) {\n\t\t\t\tlet thisClickedLink = window.location.origin + $(this).attr('href');\n\n\t\t\t\tif (!targetLink.includes(\"/\")) {\n\t\t\t\t\t// Reconstruct the URL\n\t\t\t\t\tthisClickedLink = window.location.origin + window.location.pathname + $(this).attr('href');\n\t\t\t\t}\n\n\t\t\t\tif (targetLink.includes(\"//\")) {\n\t\t\t\t\t// external link\n\t\t\t\t} else if (thisClickedLink.includes(addressBarLink)) {\n\t\t\t\t\te.preventDefault();\n\t\t\t\t\tanimateScrollPosition($(this).attr('href'));\n\t\t\t\t}\n\t\t\t}\n\t\t});\n\n\t\tfunction changeScrollPosition(top) {\n\t\t\twindow.scroll({\n\t\t\t\ttop: top,\n\t\t\t\tbehavior: 'smooth'\n\t\t\t});\n\t\t}\n\t\tfunction animateScro